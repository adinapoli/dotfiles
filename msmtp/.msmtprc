port 587
protocol smtp
auth on
from alfredo.dinapoli@gmail.com
user alfredo.dinapoli@gmail.com
passwordeval "gpg --no-tty -dq /Users/adinapoli/Dropbox/dotfiles/.authinfo.json.gpg | jq -r .email.personal.password"
tls on
tls_trust_file /usr/local/etc/openssl/cert.pem
logfile        ~/.msmtp.log

